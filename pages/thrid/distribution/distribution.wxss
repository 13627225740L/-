/* 订单列表 */

.orderlist {
 display: flex;
 align-items: center;
 width: 100%;
 background-color: #fff;
}

.orderlist text {
 margin-top: 20rpx;
 margin-left: 34rpx;
 font-size: 30rpx;
 line-height: 44rpx;
}

.imgs {
 display: flex;
 margin-top: 5rpx;
 width: 100%;
 height: 150rpx;
 justify-content: center;
 align-items: center;
 flex-direction: row;
 background-color: #fff;
}

.imag {
 width: 90rpx;
 height: 90rpx;
 border-radius: 50%;
}

.self {
 margin-left: 10rpx;
 width: 250rpx;
 height: 80rpx;
}

.name {
 color: #636363;
 font-size: 28rpx;
}

.seats {
 display: flex;
 flex-direction: row;
}

.seats image {
 width: 60rpx;
 height: 40rpx;
}

.seats text {
 font-size: 26rpx;
 color: #8f7729;
}

.money {
 display: flex;
 justify-content: center;
 align-items: center;
 flex-direction: row;
 width: 250rpx;
 height: 40rpx;
}

.moneyTA {
 color: #ccc;
 font-size: 30rpx;
}

.yuan {
 font-size: 30rpx;
 margin-left: 8rpx;
 font-weight: bold;
}

.moneyyuan,
.yuan,
.yuans {
 color: #ff5733;
}

.moneyyuan {
 margin-left: 8rpx;
 font-size: 24rpx;
}

.yuans {
 font-size: 30rpx;
}

.myself {
 display: flex;
 justify-content: center;
 align-items: center;
 width: 110rpx;
 font-size: 30rpx;
}

.mys {
 display: flex;
 justify-content: center;
 align-items: center;
 width: 100rpx;
 height: 40rpx;
 color: #ff5733;
 border: solid 1rpx #ff5733;
 border-radius: 30rpx;
}

.ok{
 display: flex;
 justify-content: center;
 align-items: center;
 width: 100rpx;
 height: 40rpx;
 color: #a6a6a6;
 border: solid 1rpx #a6a6a6;
 border-radius: 30rpx; 
}