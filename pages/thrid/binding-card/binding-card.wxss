/* pages/second/bing_card/bing_card.wxss */
page {
  background-color: #f5f5f5;
}
.container {
  padding: 0rpx;
}

.enroll-item {
  display: flex;
  flex-direction: row;
  background-color: white;
  width: 750rpx;
  height: 80rpx;
  flex-wrap: nowrap;
  align-items: center;
}

.item-title {
  font-size: 30rpx;
  color: #8a8a8a;
  white-space: nowrap;
  margin-left: 24rpx;
}

.item-xing {
  color: #fc6262;
  padding-left: 20rpx;
}

.item-content {
  font-size: 28rpx;
  color: #8a8a8a;
  flex: 1;
  text-align: right;
  padding-right: 24rpx;
}
.withdraw-note {
  font-size: 30rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.bank-reminder {
  font-size: 28rpx;
  color: #8a8a8a;
  padding: 10rpx 20rpx ;
}
.sign-up {
  font-size: 30rpx;
  height: 96rpx;
  display: flex;
  margin-top: 50rpx;
  justify-content: center;
  align-items: center;
  background-color: #3dc2ed;
  width: 600rpx;
  margin-left: 75rpx;
  border-radius: 50rpx;
  color: white;
}
